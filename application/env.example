# Node.js Environment
NODE_ENV=development
PORT=3000

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_IMAGE_MODEL=dall-e-3

# Firebase Configuration
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour Firebase private key here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your_firebase_client_email@your-project.iam.gserviceaccount.com

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/ai_framework_db
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=ai_framework_db
DATABASE_USER=username
DATABASE_PASSWORD=password

# Vector Database Configuration
VECTOR_DIMENSION=1536
PGVECTOR_ENABLED=true

# Admin Interface Configuration
ADMIN_PORT=3001
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_admin_password

# Security Configuration
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_KEY=your_32_character_encryption_key

# External Services (Optional)
REDIS_URL=redis://localhost:6379
WEBHOOK_URL=https://your-domain.com/webhooks

# Development/Debug
DEBUG_MODE=true
LOG_LEVEL=info
